<div id="<%= dom_id printer %>" class="grid grid-cols-3 gap-4 w-full">
  <%= render partial: 'printers/cards/printer_card', locals: {printer: printer} %>

  <%= render partial: 'printers/cards/extruder_card', locals: {extruder: printer.extruder} %>

  <%= render partial: 'printers/cards/action_card' %>

  <div class="card bg-base-200 shadow-xl">
    <div class="card-body">
      <h2 class="card-title">Config</h2>
      <p><code class="w-96 h-96 whitespace-pre" ><%= prepare_config(@printer) %></code></p>
    </div>
  </div>
</div>


